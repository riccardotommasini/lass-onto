<?xml version="1.0"?>


<!DOCTYPE rdf:RDF [
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY dc "http://purl.org/dc/elements/1.1/" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY xml "http://www.w3.org/XML/1998/namespace" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
]>


<rdf:RDF xmlns="http://streamreasoning.org/onto/lass.owl#"
     xml:base="http://streamreasoning.org/onto/lass.owl"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace">
    <owl:Ontology rdf:about="http://streamreasoning.org/onto/lass.owl">
        <rdfs:label>Lass Ontology</rdfs:label>
        <owl:versionInfo>Lass owl ontollogy, version 17 April 2016</owl:versionInfo>
        <rdfs:comment>A social media ontology for Stream Reasoning tests</rdfs:comment>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.w3.org/2000/01/rdf-schema#comment -->

    <owl:AnnotationProperty rdf:about="&rdfs;comment"/>
    


    <!-- http://www.w3.org/2000/01/rdf-schema#label -->

    <owl:AnnotationProperty rdf:about="&rdfs;label"/>
    


    <!-- http://www.w3.org/2002/07/owl#deprecated -->

    <owl:AnnotationProperty rdf:about="&owl;deprecated"/>
    


    <!-- http://www.w3.org/2002/07/owl#priorVersion -->

    <owl:AnnotationProperty rdf:about="&owl;priorVersion"/>
    


    <!-- http://www.w3.org/2002/07/owl#versionInfo -->

    <owl:AnnotationProperty rdf:about="&owl;versionInfo"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.w3.org/1999/02/22-rdf-syntax-ns#PlainLiteral -->

    <rdfs:Datatype rdf:about="&rdf;PlainLiteral"/>
    


    <!-- http://www.w3.org/2001/XMLSchema#boolean -->

    <rdfs:Datatype rdf:about="&xsd;boolean"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://streamreasoning.org/onto/lass.owl#contains -->

    <owl:ObjectProperty rdf:about="http://streamreasoning.org/onto/lass.owl#contains">
        <rdfs:comment>prev None</rdfs:comment>
        <rdfs:domain rdf:resource="http://streamreasoning.org/onto/lass.owl#Action"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://streamreasoning.org/onto/lass.owl#expertIn -->

    <owl:ObjectProperty rdf:about="http://streamreasoning.org/onto/lass.owl#expertIn">
        <rdfs:label>is expert in</rdfs:label>
        <rdfs:comment>prev teachingAssistantOf</rdfs:comment>
        <rdfs:domain rdf:resource="http://streamreasoning.org/onto/lass.owl#Expert"/>
        <rdfs:range rdf:resource="http://streamreasoning.org/onto/lass.owl#Topic"/>
    </owl:ObjectProperty>
    


    <!-- http://streamreasoning.org/onto/lass.owl#hasEventTag -->

    <owl:ObjectProperty rdf:about="http://streamreasoning.org/onto/lass.owl#hasEventTag">
        <rdfs:label>has event tag</rdfs:label>
        <rdfs:comment>prev researchProject</rdfs:comment>
        <rdfs:domain rdf:resource="http://streamreasoning.org/onto/lass.owl#Event"/>
        <rdfs:range rdf:resource="http://streamreasoning.org/onto/lass.owl#Tag"/>
    </owl:ObjectProperty>
    


    <!-- http://streamreasoning.org/onto/lass.owl#hasMember -->

    <owl:ObjectProperty rdf:about="http://streamreasoning.org/onto/lass.owl#hasMember">
        <rdfs:label>has member</rdfs:label>
        <rdfs:comment>prev hasAlumnus</rdfs:comment>
        <rdfs:domain rdf:resource="http://streamreasoning.org/onto/lass.owl#Site"/>
        <rdfs:range rdf:resource="http://streamreasoning.org/onto/lass.owl#User"/>
    </owl:ObjectProperty>
    


    <!-- http://streamreasoning.org/onto/lass.owl#interestedIn -->

    <owl:ObjectProperty rdf:about="http://streamreasoning.org/onto/lass.owl#interestedIn">
        <rdfs:label>is interested in</rdfs:label>
        <rdfs:comment>prev takesCourse</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://streamreasoning.org/onto/lass.owl#involvedIn -->

    <owl:ObjectProperty rdf:about="http://streamreasoning.org/onto/lass.owl#involvedIn">
        <rdfs:label>is involved in</rdfs:label>
        <rdfs:comment>prev memberOF
</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://streamreasoning.org/onto/lass.owl#isActiveMemberOf -->

    <owl:ObjectProperty rdf:about="http://streamreasoning.org/onto/lass.owl#isActiveMemberOf">
        <rdfs:label>is active member of</rdfs:label>
        <rdfs:comment>prev doctoralDegreeFrom</rdfs:comment>
        <rdfs:range rdf:resource="http://streamreasoning.org/onto/lass.owl#Site"/>
        <rdfs:domain rdf:resource="http://streamreasoning.org/onto/lass.owl#User"/>
        <rdfs:subPropertyOf rdf:resource="http://streamreasoning.org/onto/lass.owl#isMemberOf"/>
    </owl:ObjectProperty>
    


    <!-- http://streamreasoning.org/onto/lass.owl#isFollowedBy -->

    <owl:ObjectProperty rdf:about="http://streamreasoning.org/onto/lass.owl#isFollowedBy">
        <rdfs:label>is followed by</rdfs:label>
        <rdfs:comment>prev tenured</rdfs:comment>
        <rdfs:domain rdf:resource="http://streamreasoning.org/onto/lass.owl#Influencer"/>
    </owl:ObjectProperty>
    


    <!-- http://streamreasoning.org/onto/lass.owl#isInfluencedBy -->

    <owl:ObjectProperty rdf:about="http://streamreasoning.org/onto/lass.owl#isInfluencedBy">
        <rdf:type rdf:resource="&owl;TransitiveProperty"/>
        <rdfs:label>is being advised by</rdfs:label>
        <rdfs:comment>prev isadvisedby</rdfs:comment>
        <rdfs:range rdf:resource="http://streamreasoning.org/onto/lass.owl#Influencer"/>
        <rdfs:domain rdf:resource="http://streamreasoning.org/onto/lass.owl#User"/>
    </owl:ObjectProperty>
    


    <!-- http://streamreasoning.org/onto/lass.owl#isMemberOf -->

    <owl:ObjectProperty rdf:about="http://streamreasoning.org/onto/lass.owl#isMemberOf">
        <rdfs:label>is member of</rdfs:label>
        <rdfs:comment>prev hasDegreeFrom
</rdfs:comment>
        <rdfs:range rdf:resource="http://streamreasoning.org/onto/lass.owl#Site"/>
        <rdfs:domain rdf:resource="http://streamreasoning.org/onto/lass.owl#User"/>
        <owl:inverseOf rdf:resource="http://streamreasoning.org/onto/lass.owl#hasMember"/>
    </owl:ObjectProperty>
    


    <!-- http://streamreasoning.org/onto/lass.owl#isProminentOf -->

    <owl:ObjectProperty rdf:about="http://streamreasoning.org/onto/lass.owl#isProminentOf">
        <rdfs:label>is prominent member of</rdfs:label>
        <rdfs:comment>prev undergraduateDegreeFrom</rdfs:comment>
        <rdfs:range rdf:resource="http://streamreasoning.org/onto/lass.owl#Site"/>
        <rdfs:domain rdf:resource="http://streamreasoning.org/onto/lass.owl#User"/>
        <rdfs:subPropertyOf rdf:resource="http://streamreasoning.org/onto/lass.owl#isMemberOf"/>
    </owl:ObjectProperty>
    


    <!-- http://streamreasoning.org/onto/lass.owl#isReactionTo -->

    <owl:ObjectProperty rdf:about="http://streamreasoning.org/onto/lass.owl#isReactionTo">
        <rdf:type rdf:resource="&owl;TransitiveProperty"/>
        <rdfs:label>is reaction to</rdfs:label>
        <rdfs:comment>prev softwareVersion
not transitive</rdfs:comment>
        <rdfs:domain rdf:resource="http://streamreasoning.org/onto/lass.owl#Reaction"/>
    </owl:ObjectProperty>
    


    <!-- http://streamreasoning.org/onto/lass.owl#isSharing -->

    <owl:ObjectProperty rdf:about="http://streamreasoning.org/onto/lass.owl#isSharing">
        <rdf:type rdf:resource="&owl;TransitiveProperty"/>
        <rdfs:label>is sharing</rdfs:label>
        <rdfs:comment>prev softwaredocumentation</rdfs:comment>
        <rdfs:range rdf:resource="http://streamreasoning.org/onto/lass.owl#Action"/>
        <rdfs:domain rdf:resource="http://streamreasoning.org/onto/lass.owl#Reaction"/>
    </owl:ObjectProperty>
    


    <!-- http://streamreasoning.org/onto/lass.owl#isTaggedWith -->

    <owl:ObjectProperty rdf:about="http://streamreasoning.org/onto/lass.owl#isTaggedWith">
        <rdfs:label>is tagged with</rdfs:label>
        <rdfs:comment>prev publicationResearch
not subClass of contains</rdfs:comment>
        <rdfs:domain rdf:resource="http://streamreasoning.org/onto/lass.owl#Action"/>
        <rdfs:range rdf:resource="http://streamreasoning.org/onto/lass.owl#Tag"/>
        <rdfs:subPropertyOf rdf:resource="http://streamreasoning.org/onto/lass.owl#contains"/>
    </owl:ObjectProperty>
    


    <!-- http://streamreasoning.org/onto/lass.owl#isTechnicalMemberOf -->

    <owl:ObjectProperty rdf:about="http://streamreasoning.org/onto/lass.owl#isTechnicalMemberOf">
        <rdfs:label>is technical  member of</rdfs:label>
        <rdfs:comment>prev mastersDegreeFrom</rdfs:comment>
        <rdfs:range rdf:resource="http://streamreasoning.org/onto/lass.owl#Site"/>
        <rdfs:domain rdf:resource="http://streamreasoning.org/onto/lass.owl#User"/>
        <rdfs:subPropertyOf rdf:resource="http://streamreasoning.org/onto/lass.owl#isMemberOf"/>
    </owl:ObjectProperty>
    


    <!-- http://streamreasoning.org/onto/lass.owl#isVisitedBy -->

    <owl:ObjectProperty rdf:about="http://streamreasoning.org/onto/lass.owl#isVisitedBy">
        <rdfs:label>is visited by</rdfs:label>
        <rdfs:comment>prev member</rdfs:comment>
        <rdfs:domain rdf:resource="http://streamreasoning.org/onto/lass.owl#Community"/>
        <rdfs:range rdf:resource="http://streamreasoning.org/onto/lass.owl#User"/>
        <owl:inverseOf rdf:resource="http://streamreasoning.org/onto/lass.owl#involvedIn"/>
    </owl:ObjectProperty>
    


    <!-- http://streamreasoning.org/onto/lass.owl#leads -->

    <owl:ObjectProperty rdf:about="http://streamreasoning.org/onto/lass.owl#leads">
        <rdfs:label>leads</rdfs:label>
        <rdfs:subPropertyOf rdf:resource="http://streamreasoning.org/onto/lass.owl#participates"/>
    </owl:ObjectProperty>
    


    <!-- http://streamreasoning.org/onto/lass.owl#participates -->

    <owl:ObjectProperty rdf:about="http://streamreasoning.org/onto/lass.owl#participates">
        <rdfs:label>participates</rdfs:label>
        <rdfs:subPropertyOf rdf:resource="http://streamreasoning.org/onto/lass.owl#involvedIn"/>
    </owl:ObjectProperty>
    


    <!-- http://streamreasoning.org/onto/lass.owl#perfomerdOn -->

    <owl:ObjectProperty rdf:about="http://streamreasoning.org/onto/lass.owl#perfomerdOn">
        <rdfs:label>performed on</rdfs:label>
        <rdfs:comment>previous publicationDate</rdfs:comment>
        <rdfs:domain rdf:resource="http://streamreasoning.org/onto/lass.owl#Action"/>
    </owl:ObjectProperty>
    


    <!-- http://streamreasoning.org/onto/lass.owl#performedBy -->

    <owl:ObjectProperty rdf:about="http://streamreasoning.org/onto/lass.owl#performedBy">
        <rdfs:label>performed by</rdfs:label>
        <rdfs:comment>previous publicationAuthor</rdfs:comment>
        <rdfs:domain rdf:resource="http://streamreasoning.org/onto/lass.owl#Action"/>
        <rdfs:range rdf:resource="http://streamreasoning.org/onto/lass.owl#User"/>
    </owl:ObjectProperty>
    


    <!-- http://streamreasoning.org/onto/lass.owl#postsAbout -->

    <owl:ObjectProperty rdf:about="http://streamreasoning.org/onto/lass.owl#postsAbout">
        <rdfs:label>posts about</rdfs:label>
        <rdfs:comment>prev teacherOf</rdfs:comment>
        <rdfs:domain rdf:resource="http://streamreasoning.org/onto/lass.owl#Contributor"/>
        <rdfs:range rdf:resource="http://streamreasoning.org/onto/lass.owl#Topic"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description>
                <owl:inverseOf rdf:resource="http://streamreasoning.org/onto/lass.owl#performedBy"/>
            </rdf:Description>
            <rdf:Description rdf:about="http://streamreasoning.org/onto/lass.owl#contains"/>
        </owl:propertyChainAxiom>
    </owl:ObjectProperty>
    


    <!-- http://streamreasoning.org/onto/lass.owl#subCommunityOf -->

    <owl:ObjectProperty rdf:about="http://streamreasoning.org/onto/lass.owl#subCommunityOf">
        <rdf:type rdf:resource="&owl;TransitiveProperty"/>
        <rdfs:label>is sub community of</rdfs:label>
        <rdfs:comment>previous lubm:subOrganizationOf</rdfs:comment>
        <rdfs:range rdf:resource="http://streamreasoning.org/onto/lass.owl#Community"/>
        <rdfs:domain rdf:resource="http://streamreasoning.org/onto/lass.owl#Community"/>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/2002/07/owl#topObjectProperty -->

    <owl:ObjectProperty rdf:about="&owl;topObjectProperty"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://streamreasoning.org/onto/lass.owl#age -->

    <owl:DatatypeProperty rdf:about="http://streamreasoning.org/onto/lass.owl#age">
        <rdfs:label>is age</rdfs:label>
        <rdfs:domain rdf:resource="http://streamreasoning.org/onto/lass.owl#User"/>
    </owl:DatatypeProperty>
    


    <!-- http://streamreasoning.org/onto/lass.owl#emailAddress -->

    <owl:DatatypeProperty rdf:about="http://streamreasoning.org/onto/lass.owl#emailAddress">
        <rdfs:label>email</rdfs:label>
        <rdfs:domain rdf:resource="http://streamreasoning.org/onto/lass.owl#User"/>
    </owl:DatatypeProperty>
    


    <!-- http://streamreasoning.org/onto/lass.owl#followersNumber -->

    <owl:DatatypeProperty rdf:about="http://streamreasoning.org/onto/lass.owl#followersNumber">
        <rdfs:label>followers</rdfs:label>
        <rdfs:domain rdf:resource="http://streamreasoning.org/onto/lass.owl#User"/>
    </owl:DatatypeProperty>
    


    <!-- http://streamreasoning.org/onto/lass.owl#followingNumber -->

    <owl:DatatypeProperty rdf:about="http://streamreasoning.org/onto/lass.owl#followingNumber">
        <rdfs:label>following number</rdfs:label>
        <rdfs:domain rdf:resource="http://streamreasoning.org/onto/lass.owl#User"/>
    </owl:DatatypeProperty>
    


    <!-- http://streamreasoning.org/onto/lass.owl#friends -->

    <owl:DatatypeProperty rdf:about="http://streamreasoning.org/onto/lass.owl#friends">
        <rdfs:label>firends number</rdfs:label>
        <rdfs:domain rdf:resource="http://streamreasoning.org/onto/lass.owl#User"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topDataProperty"/>
    </owl:DatatypeProperty>
    


    <!-- http://streamreasoning.org/onto/lass.owl#username -->

    <owl:DatatypeProperty rdf:about="http://streamreasoning.org/onto/lass.owl#username">
        <rdfs:label>username</rdfs:label>
        <rdfs:domain rdf:resource="http://streamreasoning.org/onto/lass.owl#User"/>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://streamreasoning.org/onto/lass.owl#Action -->

    <owl:Class rdf:about="http://streamreasoning.org/onto/lass.owl#Action">
        <rdfs:label>Action</rdfs:label>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://streamreasoning.org/onto/lass.owl#performedBy"/>
                <owl:onClass rdf:resource="http://streamreasoning.org/onto/lass.owl#User"/>
                <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:seeAlso>http://schema.org/Action</rdfs:seeAlso>
        <rdfs:comment>An action performed by an user within a community</rdfs:comment>
        <rdfs:comment>previous lubm:Publication
</rdfs:comment>
    </owl:Class>
    


    <!-- http://streamreasoning.org/onto/lass.owl#Attendee -->

    <owl:Class rdf:about="http://streamreasoning.org/onto/lass.owl#Attendee">
        <rdfs:label>Attendee</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://streamreasoning.org/onto/lass.owl#User"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://streamreasoning.org/onto/lass.owl#participates"/>
                <owl:someValuesFrom rdf:resource="http://streamreasoning.org/onto/lass.owl#Event"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>an user who attends an event</rdfs:comment>
        <rdfs:comment>prev ResearchAssistant</rdfs:comment>
    </owl:Class>
    


    <!-- http://streamreasoning.org/onto/lass.owl#Comment -->

    <owl:Class rdf:about="http://streamreasoning.org/onto/lass.owl#Comment">
        <rdfs:label>Comment</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://streamreasoning.org/onto/lass.owl#Post"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://streamreasoning.org/onto/lass.owl#isReactionTo"/>
                        <owl:someValuesFrom rdf:resource="http://streamreasoning.org/onto/lass.owl#Action"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://streamreasoning.org/onto/lass.owl#Action"/>
        <rdfs:seeAlso>http://rdfs.org/sioc/types#Comment</rdfs:seeAlso>
        <rdfs:comment>Specific kind of posts that belongs to other post to exist</rdfs:comment>
        <rdfs:comment>Previous Book</rdfs:comment>
    </owl:Class>
    


    <!-- http://streamreasoning.org/onto/lass.owl#Commercial -->

    <owl:Class rdf:about="http://streamreasoning.org/onto/lass.owl#Commercial">
        <rdfs:subClassOf rdf:resource="http://streamreasoning.org/onto/lass.owl#Influencer"/>
        <rdfs:comment>An user account that belongs to a commercial activity e.g. a page on Facebook</rdfs:comment>
        <rdfs:comment>prev AssistantProfessor</rdfs:comment>
        <rdfs:comment>Commercial</rdfs:comment>
    </owl:Class>
    


    <!-- http://streamreasoning.org/onto/lass.owl#Community -->

    <owl:Class rdf:about="http://streamreasoning.org/onto/lass.owl#Community">
        <rdfs:label>Community</rdfs:label>
        <rdfs:comment>A Community may consist of people, sites, discussion, topics, threads, events joined by a common topic, interests or goals.

</rdfs:comment>
        <rdfs:seeAlso>http://rdfs.org/sioc/spec/#term_Community</rdfs:seeAlso>
        <rdfs:comment>previous lubm:organization
</rdfs:comment>
    </owl:Class>
    


    <!-- http://streamreasoning.org/onto/lass.owl#Concept -->

    <owl:Class rdf:about="http://streamreasoning.org/onto/lass.owl#Concept">
        <rdfs:label>Concept</rdfs:label>
        <rdfs:comment>previous lubm:Work</rdfs:comment>
    </owl:Class>
    


    <!-- http://streamreasoning.org/onto/lass.owl#Contributor -->

    <owl:Class rdf:about="http://streamreasoning.org/onto/lass.owl#Contributor">
        <rdfs:label>Contributor</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://streamreasoning.org/onto/lass.owl#Prominent"/>
        <rdfs:comment>an user who actively participates into a community</rdfs:comment>
        <rdfs:comment>prev Faculty</rdfs:comment>
    </owl:Class>
    


    <!-- http://streamreasoning.org/onto/lass.owl#Discussion -->

    <owl:Class rdf:about="http://streamreasoning.org/onto/lass.owl#Discussion">
        <rdfs:label>Discussion</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://streamreasoning.org/onto/lass.owl#Community"/>
        <rdfs:comment>In a discussion users have the option of responding to one another directly. 
There may be a general topic. A discussion is a group conversation with simultaneous side conversations.</rdfs:comment>
    </owl:Class>
    


    <!-- http://streamreasoning.org/onto/lass.owl#DiscussionLeader -->

    <owl:Class rdf:about="http://streamreasoning.org/onto/lass.owl#DiscussionLeader">
        <rdfs:label>DiscussionLeader</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://streamreasoning.org/onto/lass.owl#User"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://streamreasoning.org/onto/lass.owl#leads"/>
                        <owl:someValuesFrom rdf:resource="http://streamreasoning.org/onto/lass.owl#Discussion"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://streamreasoning.org/onto/lass.owl#Influencer"/>
        <rdfs:comment>Person who introduces topic of discussion and leads the discussion.</rdfs:comment>
        <rdfs:comment>prev Chair</rdfs:comment>
    </owl:Class>
    


    <!-- http://streamreasoning.org/onto/lass.owl#Event -->

    <owl:Class rdf:about="http://streamreasoning.org/onto/lass.owl#Event">
        <rdfs:label>Event</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://streamreasoning.org/onto/lass.owl#Community"/>
        <rdfs:comment>prev ResearchGroup</rdfs:comment>
        <rdfs:comment>something that occurs in a certain place during a particular time:</rdfs:comment>
        <rdfs:seeAlso>http://schema.org/Event</rdfs:seeAlso>
    </owl:Class>
    


    <!-- http://streamreasoning.org/onto/lass.owl#Expert -->

    <owl:Class rdf:about="http://streamreasoning.org/onto/lass.owl#Expert">
        <rdfs:label>Expert</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://streamreasoning.org/onto/lass.owl#User"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://streamreasoning.org/onto/lass.owl#expertIn"/>
                        <owl:someValuesFrom rdf:resource="http://streamreasoning.org/onto/lass.owl#Topic"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:comment>prev university teaching assistant</rdfs:comment>
        <rdfs:comment>a person who has special skill or knowledge in a particular field.</rdfs:comment>
    </owl:Class>
    


    <!-- http://streamreasoning.org/onto/lass.owl#Influencer -->

    <owl:Class rdf:about="http://streamreasoning.org/onto/lass.owl#Influencer">
        <rdfs:label>Influencer</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://streamreasoning.org/onto/lass.owl#Contributor"/>
        <rdfs:comment>An user who can influence other users in the community</rdfs:comment>
        <rdfs:comment>prev Professor</rdfs:comment>
    </owl:Class>
    


    <!-- http://streamreasoning.org/onto/lass.owl#Involved -->

    <owl:Class rdf:about="http://streamreasoning.org/onto/lass.owl#Involved">
        <rdfs:label>An user who is involved in a discussion.

http://www.wordreference.com/definition/involved</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://streamreasoning.org/onto/lass.owl#Contributor"/>
        <rdfs:comment>prev lecturer</rdfs:comment>
    </owl:Class>
    


    <!-- http://streamreasoning.org/onto/lass.owl#LegalContributor -->

    <owl:Class rdf:about="http://streamreasoning.org/onto/lass.owl#LegalContributor">
        <rdfs:label>Legal</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://streamreasoning.org/onto/lass.owl#TechnicalMember"/>
        <rdfs:comment>pre ClericalStaff</rdfs:comment>
