        <terms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2016-04-15T00:00:00.000+02:00</terms:issued>
        <ns:license rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://tldrlegal.com/license/mit-license</ns:license>
        <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">version 28 june 2016</owl:versionInfo>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A social media vocabulary for Stream Reasoning Benchmarking</dc:description>
        <dc:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Riccardo Tommasini - riccardo.tommasini@polimi.it
Marco Balduini - marco.balduini@polimi.it
Emanuele Della Valle - emanuele.dellavalle@polimi.it</dc:creator>
    </owl:Ontology>
    


