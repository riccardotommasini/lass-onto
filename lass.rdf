    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://streamreasoning.org/ontologies/lass.owl#contains -->

    <owl:ObjectProperty rdf:about="http://streamreasoning.org/ontologies/lass.owl#contains">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://streamreasoning.org/ontologies/lass.owl#Action"/>
        <rdfs:comment>Specifies that an Action contains another generic entity e.g. A Post containing the citation of another Post

prev None</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">contains</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://streamreasoning.org/ontologies/lass.owl#expertIn -->

    <owl:ObjectProperty rdf:about="http://streamreasoning.org/ontologies/lass.owl#expertIn">
        <rdfs:domain rdf:resource="http://streamreasoning.org/ontologies/lass.owl#Expert"/>
        <rdfs:range rdf:resource="http://streamreasoning.org/ontologies/lass.owl#Topic"/>
        <rdfs:comment>Specifies that an Expert skilled about a Topic

prev teachingAssistantOf</rdfs:comment>
        <rdfs:label>is expert in</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://streamreasoning.org/ontologies/lass.owl#hasEventTag -->

    <owl:ObjectProperty rdf:about="http://streamreasoning.org/ontologies/lass.owl#hasEventTag">
        <rdfs:domain rdf:resource="http://streamreasoning.org/ontologies/lass.owl#Event"/>
        <rdfs:range rdf:resource="http://streamreasoning.org/ontologies/lass.owl#Tag"/>
        <rdfs:comment>Specifies that an Event is marked with a Tag e.g. The hashtag #euro2016 is related to Europe cup 2016</rdfs:comment>
        <rdfs:label>has event tag</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://streamreasoning.org/ontologies/lass.owl#hasMember -->

    <owl:ObjectProperty rdf:about="http://streamreasoning.org/ontologies/lass.owl#hasMember">
        <owl:inverseOf rdf:resource="http://streamreasoning.org/ontologies/lass.owl#isMemberOf"/>
        <rdfs:domain rdf:resource="http://streamreasoning.org/ontologies/lass.owl#Site"/>
        <rdfs:range rdf:resource="http://streamreasoning.org/ontologies/lass.owl#User"/>
        <rdfs:comment>Connects a Site with the subscribed Users e.g. A User of Twitter

prev hasAlumnus</rdfs:comment>
        <rdfs:label>has member</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://streamreasoning.org/ontologies/lass.owl#interestedIn -->

    <owl:ObjectProperty rdf:about="http://streamreasoning.org/ontologies/lass.owl#interestedIn">
        <rdfs:comment>Specifies that a User is interested in something e.g. A user interested in sport content
prev takesCourse</rdfs:comment>
        <rdfs:label>is interested in</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://streamreasoning.org/ontologies/lass.owl#involvedIn -->

    <owl:ObjectProperty rdf:about="http://streamreasoning.org/ontologies/lass.owl#involvedIn">
        <owl:inverseOf rdf:resource="http://streamreasoning.org/ontologies/lass.owl#isVisitedBy"/>
        <rdfs:comment>Specifies that a User is part of a Community e.g. A User of a Site

prev memberOF</rdfs:comment>
        <rdfs:label>is involved in</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://streamreasoning.org/ontologies/lass.owl#isActiveMemberOf -->

    <owl:ObjectProperty rdf:about="http://streamreasoning.org/ontologies/lass.owl#isActiveMemberOf">
        <rdfs:subPropertyOf rdf:resource="http://streamreasoning.org/ontologies/lass.owl#isMemberOf"/>
        <rdfs:domain rdf:resource="http://streamreasoning.org/ontologies/lass.owl#Contributor"/>
        <rdfs:range rdf:resource="http://streamreasoning.org/ontologies/lass.owl#Site"/>
        <rdfs:comment>Specifies that an active User participates to a Site life

prev doctoralDegreeFrom</rdfs:comment>
        <rdfs:label>is active member of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://streamreasoning.org/ontologies/lass.owl#isFollowedBy -->

    <owl:ObjectProperty rdf:about="http://streamreasoning.org/ontologies/lass.owl#isFollowedBy">
        <rdfs:domain rdf:resource="http://streamreasoning.org/ontologies/lass.owl#Influencer"/>
        <rdfs:comment>Specifies that an Influencer is followed by other things e.g. An Influencer is followeb by another User

prev tenured</rdfs:comment>
        <rdfs:label>is followed by</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://streamreasoning.org/ontologies/lass.owl#isInfluencedBy -->

    <owl:ObjectProperty rdf:about="http://streamreasoning.org/ontologies/lass.owl#isInfluencedBy">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="http://streamreasoning.org/ontologies/lass.owl#User"/>
        <rdfs:range rdf:resource="http://streamreasoning.org/ontologies/lass.owl#Influencer"/>
        <rdfs:comment>Specifies that a User is influenced by an Influencer e.g. A User passionate in technology is influenced by Apple account (another User)

prev isadvisedby</rdfs:comment>
        <rdfs:label>is being advised by</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://streamreasoning.org/ontologies/lass.owl#isMemberOf -->

    <owl:ObjectProperty rdf:about="http://streamreasoning.org/ontologies/lass.owl#isMemberOf">
        <rdfs:domain rdf:resource="http://streamreasoning.org/ontologies/lass.owl#User"/>
        <rdfs:range rdf:resource="http://streamreasoning.org/ontologies/lass.owl#Site"/>
        <rdfs:comment>Connects Users to Sites

prev hasDegreeFrom</rdfs:comment>
        <rdfs:label>is member of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://streamreasoning.org/ontologies/lass.owl#isProminentMemberOf -->

    <owl:ObjectProperty rdf:about="http://streamreasoning.org/ontologies/lass.owl#isProminentMemberOf">
        <rdfs:subPropertyOf rdf:resource="http://streamreasoning.org/ontologies/lass.owl#isMemberOf"/>
        <rdfs:domain rdf:resource="http://streamreasoning.org/ontologies/lass.owl#Prominent"/>
        <rdfs:range rdf:resource="http://streamreasoning.org/ontologies/lass.owl#Site"/>
        <rdfs:comment>Specifies that a actively involved or technical User participates to a Site life e.g. An Influencer is prominent member of Twitter

prev undergraduateDegreeFrom</rdfs:comment>
        <rdfs:label>is prominent member of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://streamreasoning.org/ontologies/lass.owl#isReactionTo -->

    <owl:ObjectProperty rdf:about="http://streamreasoning.org/ontologies/lass.owl#isReactionTo">
        <owl:inverseOf rdf:resource="http://streamreasoning.org/ontologies/lass.owl#relatedTo"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="http://streamreasoning.org/ontologies/lass.owl#Reaction"/>
        <rdfs:range rdf:resource="http://streamreasoning.org/ontologies/lass.owl#Action"/>
        <rdfs:comment>Specifies that a Reaction is related to the Action.  e.g. A Like related to a MediaPost (Instagram image)

prev softwareVersion</rdfs:comment>
        <rdfs:label>is reaction to</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://streamreasoning.org/ontologies/lass.owl#isSharing -->

    <owl:ObjectProperty rdf:about="http://streamreasoning.org/ontologies/lass.owl#isSharing">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="http://streamreasoning.org/ontologies/lass.owl#Reaction"/>
        <rdfs:range rdf:resource="http://streamreasoning.org/ontologies/lass.owl#Action"/>
        <rdfs:comment>prev softwaredocumentation</rdfs:comment>
        <rdfs:label>is sharing</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://streamreasoning.org/ontologies/lass.owl#isTaggedWith -->

    <owl:ObjectProperty rdf:about="http://streamreasoning.org/ontologies/lass.owl#isTaggedWith">
        <rdfs:subPropertyOf rdf:resource="http://streamreasoning.org/ontologies/lass.owl#contains"/>
        <rdfs:domain rdf:resource="http://streamreasoning.org/ontologies/lass.owl#Action"/>
        <rdfs:range rdf:resource="http://streamreasoning.org/ontologies/lass.owl#Tag"/>
        <rdfs:comment>Specifies that an Action contains a Tag e.g. A Post containing a Tag

prev publicationResearch
not subClass of contains</rdfs:comment>
        <rdfs:label>is tagged with</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://streamreasoning.org/ontologies/lass.owl#isTechnicalMemberOf -->

    <owl:ObjectProperty rdf:about="http://streamreasoning.org/ontologies/lass.owl#isTechnicalMemberOf">
        <rdfs:subPropertyOf rdf:resource="http://streamreasoning.org/ontologies/lass.owl#isMemberOf"/>
        <rdfs:domain rdf:resource="http://streamreasoning.org/ontologies/lass.owl#TechnicalMember"/>
        <rdfs:range rdf:resource="http://streamreasoning.org/ontologies/lass.owl#Site"/>
        <rdfs:comment>Specifies that a technical User participates to a Site life e.g. A moderator is a technical member of a blog

prev mastersDegreeFrom</rdfs:comment>
        <rdfs:label>is technical  member of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://streamreasoning.org/ontologies/lass.owl#isVisitedBy -->

    <owl:ObjectProperty rdf:about="http://streamreasoning.org/ontologies/lass.owl#isVisitedBy">
        <rdfs:domain rdf:resource="http://streamreasoning.org/ontologies/lass.owl#Community"/>
        <rdfs:range rdf:resource="http://streamreasoning.org/ontologies/lass.owl#User"/>
        <rdfs:comment>Connects a Community to its visitors e.g. A Twitter User access Twitter Site to look its timeline

prev member</rdfs:comment>
        <rdfs:label>is visited by</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://streamreasoning.org/ontologies/lass.owl#leads -->

    <owl:ObjectProperty rdf:about="http://streamreasoning.org/ontologies/lass.owl#leads">
        <rdfs:subPropertyOf rdf:resource="http://streamreasoning.org/ontologies/lass.owl#participates"/>
        <rdfs:comment>Specifies that a User leads a Community e.g. A User is the leader of a Discussion

prev headOf</rdfs:comment>
        <rdfs:label>leads</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://streamreasoning.org/ontologies/lass.owl#participates -->

    <owl:ObjectProperty rdf:about="http://streamreasoning.org/ontologies/lass.owl#participates">
        <rdfs:subPropertyOf rdf:resource="http://streamreasoning.org/ontologies/lass.owl#involvedIn"/>
        <rdfs:comment>Specifies that a User is active in a Community e.g. A User of a Site

prev worksFor</rdfs:comment>
        <rdfs:label>participates</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://streamreasoning.org/ontologies/lass.owl#perfomedOn -->

    <owl:ObjectProperty rdf:about="http://streamreasoning.org/ontologies/lass.owl#perfomedOn">
        <rdfs:domain rdf:resource="http://streamreasoning.org/ontologies/lass.owl#Action"/>
        <rdfs:comment>previous publicationDate</rdfs:comment>
        <rdfs:label>Specifies that an Action is performed on something e.g. A Comment is performed on a Post

performed on</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://streamreasoning.org/ontologies/lass.owl#performedBy -->

    <owl:ObjectProperty rdf:about="http://streamreasoning.org/ontologies/lass.owl#performedBy">
        <rdfs:domain rdf:resource="http://streamreasoning.org/ontologies/lass.owl#Action"/>
        <rdfs:range rdf:resource="http://streamreasoning.org/ontologies/lass.owl#User"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies that an Action is performed by a User e.g. A User Posts a comment about a Post

previous publicationAuthor</rdfs:comment>
        <rdfs:label>performed by</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://streamreasoning.org/ontologies/lass.owl#postsAbout -->

    <owl:ObjectProperty rdf:about="http://streamreasoning.org/ontologies/lass.owl#postsAbout">
        <rdfs:domain rdf:resource="http://streamreasoning.org/ontologies/lass.owl#Contributor"/>
        <rdfs:range rdf:resource="http://streamreasoning.org/ontologies/lass.owl#Topic"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description>
                <owl:inverseOf rdf:resource="http://streamreasoning.org/ontologies/lass.owl#performedBy"/>
            </rdf:Description>
            <rdf:Description rdf:about="http://streamreasoning.org/ontologies/lass.owl#contains"/>
        </owl:propertyChainAxiom>
        <rdfs:comment>Specifies that a Contributor create a content related to a Topic e.g. A User on Twitter posts a tweet related to Italian Football National Team

prev teacherOf</rdfs:comment>
        <rdfs:label>posts about</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://streamreasoning.org/ontologies/lass.owl#relatedTo -->

    <owl:ObjectProperty rdf:about="http://streamreasoning.org/ontologies/lass.owl#relatedTo">
        <rdfs:domain rdf:resource="http://streamreasoning.org/ontologies/lass.owl#Action"/>
        <rdfs:range rdf:resource="http://streamreasoning.org/ontologies/lass.owl#Reaction"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Spedifies that this Action is related to a particular Reaction e.g. A Like on a Comment</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">relatedTo</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://streamreasoning.org/ontologies/lass.owl#subCommunityOf -->

    <owl:ObjectProperty rdf:about="http://streamreasoning.org/ontologies/lass.owl#subCommunityOf">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="http://streamreasoning.org/ontologies/lass.owl#Community"/>
        <rdfs:range rdf:resource="http://streamreasoning.org/ontologies/lass.owl#Community"/>
        <rdfs:comment>Connects a community to its super-Community e.g. a Discussion is a sub community of a Site

previous lubm:subOrganizationOf</rdfs:comment>
        <rdfs:label>is sub community of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/2002/07/owl#topObjectProperty -->

    <owl:ObjectProperty rdf:about="http://www.w3.org/2002/07/owl#topObjectProperty"/>
    


