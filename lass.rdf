    <owl:DatatypeProperty rdf:about="http://streamreasoning.org/ontologies/lass.owl#age">
        <rdfs:domain rdf:resource="http://streamreasoning.org/ontologies/lass.owl#User"/>
        <rdfs:label>is age</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://streamreasoning.org/ontologies/lass.owl#emailAddress -->

    <owl:DatatypeProperty rdf:about="http://streamreasoning.org/ontologies/lass.owl#emailAddress">
        <rdfs:domain rdf:resource="http://streamreasoning.org/ontologies/lass.owl#User"/>
        <rdfs:label>email</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://streamreasoning.org/ontologies/lass.owl#followersNumber -->

    <owl:DatatypeProperty rdf:about="http://streamreasoning.org/ontologies/lass.owl#followersNumber">
        <rdfs:domain rdf:resource="http://streamreasoning.org/ontologies/lass.owl#User"/>
        <rdfs:label>followers</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://streamreasoning.org/ontologies/lass.owl#followingNumber -->

    <owl:DatatypeProperty rdf:about="http://streamreasoning.org/ontologies/lass.owl#followingNumber">
        <rdfs:domain rdf:resource="http://streamreasoning.org/ontologies/lass.owl#User"/>
        <rdfs:label>following number</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://streamreasoning.org/ontologies/lass.owl#friends -->

    <owl:DatatypeProperty rdf:about="http://streamreasoning.org/ontologies/lass.owl#friends">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="http://streamreasoning.org/ontologies/lass.owl#User"/>
        <rdfs:label>firends number</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://streamreasoning.org/ontologies/lass.owl#username -->

    <owl:DatatypeProperty rdf:about="http://streamreasoning.org/ontologies/lass.owl#username">
        <rdfs:domain rdf:resource="http://streamreasoning.org/ontologies/lass.owl#User"/>
        <rdfs:label>username</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
