    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://streamreasoning.org/ontologies/lass.owl#Action -->

    <owl:Class rdf:about="http://streamreasoning.org/ontologies/lass.owl#Action">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://streamreasoning.org/ontologies/lass.owl#performedBy"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://streamreasoning.org/ontologies/lass.owl#User"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>An action performed by an user within a community</rdfs:comment>
        <rdfs:label>Action</rdfs:label>
        <rdfs:seeAlso>http://schema.org/Action</rdfs:seeAlso>
    </owl:Class>
    


    <!-- http://streamreasoning.org/ontologies/lass.owl#Attendee -->

    <owl:Class rdf:about="http://streamreasoning.org/ontologies/lass.owl#Attendee">
        <rdfs:subClassOf rdf:resource="http://streamreasoning.org/ontologies/lass.owl#User"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://streamreasoning.org/ontologies/lass.owl#participates"/>
                <owl:someValuesFrom rdf:resource="http://streamreasoning.org/ontologies/lass.owl#Event"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>An user who attends an event</rdfs:comment>
        <rdfs:label>Attendee</rdfs:label>
    </owl:Class>
    


    <!-- http://streamreasoning.org/ontologies/lass.owl#Comment -->

    <owl:Class rdf:about="http://streamreasoning.org/ontologies/lass.owl#Comment">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://streamreasoning.org/ontologies/lass.owl#Post"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://streamreasoning.org/ontologies/lass.owl#isReactionTo"/>
                        <owl:someValuesFrom rdf:resource="http://streamreasoning.org/ontologies/lass.owl#Action"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://streamreasoning.org/ontologies/lass.owl#Action"/>
        <rdfs:comment>Specific kind of posts that belongs to other post to exist</rdfs:comment>
        <rdfs:label>Comment</rdfs:label>
        <rdfs:seeAlso>http://rdfs.org/sioc/types#Comment</rdfs:seeAlso>
    </owl:Class>
    


    <!-- http://streamreasoning.org/ontologies/lass.owl#Commercial -->

    <owl:Class rdf:about="http://streamreasoning.org/ontologies/lass.owl#Commercial">
        <rdfs:subClassOf rdf:resource="http://streamreasoning.org/ontologies/lass.owl#Influencer"/>
        <rdfs:comment>An user account that belongs to a commercial activity e.g. a page on Facebook</rdfs:comment>
        <rdfs:comment>Special Influencer that act for commercialpurposes</rdfs:comment>
    </owl:Class>
    


    <!-- http://streamreasoning.org/ontologies/lass.owl#Community -->

    <owl:Class rdf:about="http://streamreasoning.org/ontologies/lass.owl#Community">
        <rdfs:comment>A Community may consist of people, sites, discussion, topics, threads, events joined by a common topic, interests or goals.

</rdfs:comment>
        <rdfs:comment>previous lubm:organization
</rdfs:comment>
        <rdfs:label>Community</rdfs:label>
        <rdfs:seeAlso>http://rdfs.org/sioc/spec/#term_Community</rdfs:seeAlso>
    </owl:Class>
    


